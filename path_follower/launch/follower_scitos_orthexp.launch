<?xml version="1.0"?>
<launch>
    <group ns="path_follower">
        <group ns="controller">
            <!-- parameters, that are dependent of the controller or the robot model -->
            <group ns="differential_orthexp">
                <param name="kp" value="2.0" />
                <param name="kd" value="1.0" />

                <param name="look_ahead_dist" value=".70" />
                <param name="k" value="1.7" />
                <param name="k_o" value="0.0" />
                <param name="k_g" value="0.0" />
                <param name="k_w" value="0.9" />
                <param name="k_curv" value="0.3" />
            </group>
        </group>
    </group>
</launch>
