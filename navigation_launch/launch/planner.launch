<?xml version="1.0" ?>
<launch>
    <node name="path_planner" type="path_planner_node" pkg="path_planner" output="screen" respawn="true">
        <param name="use_map_topic" value="false" />
		<!--<param name="map_service" value="/dynamic_map" />-->
		<param name="map_service" value="/static_map" />
		<param name="map_topic" value="/map" />
		<param name="use_unknown_cells" value="true" />
        <param name="use_cost_map" value="false" />
        <param name="preprocess" value="true" />
        <param name="postprocess" value="true" />
        <param name="use_scan_front" value="true" />
        <param name="use_scan_back" value="true" />
        <param name="use_collision_gridmap" value="false" />

        <param name="size/forward" value="0.45" />
        <param name="size/backward" value="-0.45" />
        <param name="size/width" value="0.7" />
	</node>
</launch>
